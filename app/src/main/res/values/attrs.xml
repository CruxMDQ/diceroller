<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StatBox">
        <attr name="isEditionAllowed" format="boolean" />
        <attr name="statName" format="string" />
        <attr name="statCategory" format="string" />
        <attr name="statType" format="string" />
        <attr name="statKind" format="string" />
        <attr name="statValue" format="string" />
        <attr name="statColor" format="integer" />
        <attr name="colorSelected" format="color" />
    </declare-styleable>

    <declare-styleable name="StatLayout">
        <attr name="selectedStat" format="string" />
        <attr name="isOpen" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ResourceLayout">
        <attr name="panelName" format="string" />
        <attr name="panelValue" format="integer" />
        <attr name="isPanelOpen" format="boolean" />
        <attr name="boxesHaveMultipleStates" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ResourceBox">
        <attr name="hasMultipleStates" format="boolean" />
    </declare-styleable>
</resources>